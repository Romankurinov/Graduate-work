{
  "$schema": "http://json-schema.org/draft-07/schema",
  "type": "array",
  "items": {
    "type": "object",
    "required": [
      "number",
      "month",
      "year",
      "holder",
      "cvc"
    ],
    "additionalProperties": false,
    "properties": {
      "number": {
        "type": "integer"
      },
      "month": {
        "type": "integer"
      },
      "year": {
        "type": "integer"
      },
      "holder": {
        "type": "string",
        "minLength": 1
      },
      "cvc": {
        "type": "integer"
      }
    }
  }
}